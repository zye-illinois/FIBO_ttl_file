	.

fibo-sec-dbt-ab-abs:CashAssetBackedSecurity
	a owl:Class ;
	rdfs:subClassOf [
		a owl:Restriction ;
		owl:onProperty fibo-sec-dbt-ab-mbs:hasTrancheType ;
		owl:someValuesFrom fibo-sec-dbt-ab-mbs:TrancheType ;
	] ;
	owl:disjointWith fibo-sec-dbt-ab-mbs:MortgageBackedSecurity ;
	.

fibo-sec-dbt-ab-abs:HomeEquityLineOfCreditPool
	a owl:Class ;
	owl:disjointWith fibo-sec-dbt-ab-mbs:MortgagePool ;
	.

fibo-sec-dbt-ab-mbs:AgencyMBSDeal
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-ab-mbs:MortgageBackedSecurityOffering ;
	rdfs:label "agency m b s deal"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBackedSecurities/MortgageBackedSecurities/> ;
	owl:disjointWith fibo-sec-dbt-ab-mbs:PrivateLabelMBSDeal ;
	skos:definition "An issue of securities backed by pools of mortgages held by government agencies."@en ;
	fibo-fnd-utl-av:explanatoryNote "These are Ginnie Mae, Freddie Mac and Fannie Mae (for the US). Of these agencies, GNMA (Ginnie Mae) issues mortgages in its own right (Investorwords differs on this). Fannie Mae and Freddie Mac purchase mortgages. Those mortgages are issued by banks. Before one of these agencies purchases a mortgage, there are certain criteria that have to be met. These are specified in terms of, for example, the balance of the mortgage, limits to credit ratings."@en ;
	.

fibo-sec-dbt-ab-mbs:AgencyMBSIssuer
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-ab-mbs:GovernmentMortgageAgency ,
		fibo-sec-dbt-ab-mbs:MBSIssuer
		;
	rdfs:label "agency m b s issuer"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBackedSecurities/MortgageBackedSecurities/> ;
	skos:definition "The issuer of the pass through MBS is an Agency issuer. This is identified as being some kind of agency that is set up specifically to issue these instruments."@en ;
	.

fibo-sec-dbt-ab-mbs:AgencyMBSPool
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-ab-mbs:MBSPool ;
	rdfs:label "agency m b s pool"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBackedSecurities/MortgageBackedSecurities/> ;
	skos:definition "A pool investment consisting of a collection of Agency MBS instruments."@en ;
	fibo-fnd-utl-av:explanatoryNote "This pool may be used as the underlying for an agency CMO. Non agency CMOs do not exist."@en ;
	.

fibo-sec-dbt-ab-mbs:AgencyMortgagePool
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-ab-mbs:MortgagePool ;
	rdfs:label "agency mortgage pool"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBackedSecurities/MortgageBackedSecurities/> ;
	owl:disjointWith fibo-sec-dbt-ab-mbs:NonAgencyMortgagePool ;
	skos:definition "A pool of agency mortgages."@en ;
	.

fibo-sec-dbt-ab-mbs:CommercialMBS
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-ab-mbs:MortgageBackedSecurity ;
	rdfs:label "commercial m b s"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBackedSecurities/MortgageBackedSecurities/> ;
	owl:disjointWith fibo-sec-dbt-ab-mbs:ResidentialMBS ;
	.

fibo-sec-dbt-ab-mbs:FHLMC-GoldPool
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-ab-mbs:AgencyMortgagePool ;
	rdfs:label "FHLMC Gold pool"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBackedSecurities/MortgageBackedSecurities/> ;
	.

fibo-sec-dbt-ab-mbs:FHLMC-Pool
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-ab-mbs:AgencyMortgagePool ;
	rdfs:label "FHLMC pool"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBackedSecurities/MortgageBackedSecurities/> ;
	.

fibo-sec-dbt-ab-mbs:FNMA-Pool
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-ab-mbs:AgencyMortgagePool ;
	rdfs:label "FNMA pool"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBackedSecurities/MortgageBackedSecurities/> ;
	.

fibo-sec-dbt-ab-mbs:FloaterTranche
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-ab-mbs:TranchedMBSInstrument ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-dt-fd:hasRecurrenceInterval ;
			owl:someValuesFrom fibo-fnd-dt-bd:DayOfMonth ;
		]
		;
	rdfs:label "floater tranche"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBackedSecurities/MortgageBackedSecurities/> ;
	skos:definition "A floater tranche is a tranche that is keyed to an index and a spread."@en ;
	fibo-fnd-utl-av:explanatoryNote "For example, 3 month LIBOR +50 -- meaning that the coupon would be whatever the 3 month LIBOR is plus 50 basis points. This is not a continuously updated number, rather it resets at specified intervals."@en ;
	.

fibo-sec-dbt-ab-mbs:GNMA-IIPool
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-ab-mbs:AgencyMortgagePool ;
	rdfs:label "GNMA-II pool"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBackedSecurities/MortgageBackedSecurities/> ;
	.

fibo-sec-dbt-ab-mbs:GNMA-PlatinumPool
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-ab-mbs:AgencyMortgagePool ;
	rdfs:label "GNMA Platinum pool"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBackedSecurities/MortgageBackedSecurities/> ;
	.

fibo-sec-dbt-ab-mbs:GNMA-iPool
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-ab-mbs:AgencyMortgagePool ;
	rdfs:label "GNMA-i pool"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBackedSecurities/MortgageBackedSecurities/> ;
	.

fibo-sec-dbt-ab-mbs:GovernmentMortgageAgency
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-fct-fct:FunctionalEntity ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
			owl:someValuesFrom fibo-be-le-lp:LegalEntity ;
		]
		;
	rdfs:label "government mortgage agency"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBackedSecurities/MortgageBackedSecurities/> ;
	skos:definition "An agency set up by a government for the purpose of issuing mortgages."@en ;
	skos:editorialNote "Note on the original property 'takes form of'  (replaced by a restriction): The form which the Government Mortgage Agency takes. This is a Legal Entity and not a Natural Person, that is, it is an Artificial Person."@en ;
	skos:scopeNote "These exist in the USA and may also exist now or in future in other countries. There are three such agencies in thr USA: FNMA, GNMA and FHLMC (Fannie Mae, Ginnie Mae and Freddie Mac respectively)."@en ;
	.

fibo-sec-dbt-ab-mbs:InverseFloaterTranche
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-ab-mbs:FloaterTranche ;
	rdfs:label "inverse floater tranche"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBackedSecurities/MortgageBackedSecurities/> ;
	owl:disjointWith
		fibo-sec-dbt-ab-mbs:RegularFloaterTranche ,
		fibo-sec-dbt-ab-mbs:SuperFloaterTranche
		;
	.

fibo-sec-dbt-ab-mbs:IssuedAndSecuritizedAgencyMortagePool
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-ab-mbs:AgencyMortgagePool ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-arr-lif:hasStage ;
			owl:someValuesFrom fibo-bp-iss-dbti:Issued ;
		]
		;
	rdfs:label "issued and securitized agency mortage pool"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBackedSecurities/MortgageBackedSecurities/> ;
	skos:definition "An agency mortgage pool which has been securitized as part of an agency Mortgage Backed Security."@en ;
	.

fibo-sec-dbt-ab-mbs:IssuedAndSecuritizedNonAgencyMortgagePool
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-ab-mbs:NonAgencyMortgagePool ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-arr-lif:hasStage ;
			owl:someValuesFrom fibo-bp-iss-dbti:Issued ;
		]
		;
	rdfs:label "issued and securitized non agency mortgage pool"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBackedSecurities/MortgageBackedSecurities/> ;
	skos:definition "A non agency mortgage pool which has been securitized as part of a tranched Mortgage Backed Security."@en ;
	.

fibo-sec-dbt-ab-mbs:MBSIssuer
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fi-fi:Issuer ,
		[
			a owl:Class ;
			owl:unionOf (
				fibo-sec-dbt-ab-mbs:GovernmentMortgageAgency
				fibo-fbc-fct-fse:Bank
				fibo-sec-dbt-ab-mbs:SpecialistMortgageIssuer
			) ;
		]
		;
	rdfs:label "m b s issuer"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBackedSecurities/MortgageBackedSecurities/> ;
	skos:definition "The issuer of a Mortgage Backed Security."@en ;
	fibo-fnd-utl-av:explanatoryNote "This can be an agency that exists for this purpose or it may be the issuer of the original mortgages in the pool."@en ;
	.

fibo-sec-dbt-ab-mbs:MBSPool
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-ab-pbs:CashStructuredFinanceInstrumentPool ;
	rdfs:label "m b s pool"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBackedSecurities/MortgageBackedSecurities/> ;
	skos:definition "A pool investment consisting of a collection of MBS instruments."@en ;
	.

fibo-sec-dbt-ab-mbs:MBSTrancheNote
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-ab-mbs:SecurityNote ;
	rdfs:label "m b s tranche note"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBackedSecurities/MortgageBackedSecurities/> ;
	skos:definition "An individual note of a tranche."@en ;
	fibo-fnd-utl-av:explanatoryNote "A Tranche is made up of e.g. $500m in notes and so on. These may be in different notes, with different denominations. Analytics that would apply to the Tranche would by implication apply to each slice of the tranche."@en ;
	.

fibo-sec-dbt-ab-mbs:MezzanineMBSTranche
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-ab-mbs:TranchedMBSInstrument ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-ab-mbs:seniorTo ;
			owl:someValuesFrom fibo-sec-dbt-ab-mbs:SubordinatedMBSTranche ;
		]
		;
	rdfs:label "mezzanine m b s tranche"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBackedSecurities/MortgageBackedSecurities/> ;
