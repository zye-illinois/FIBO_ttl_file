					rdf:rest [
						a rdf:List ;
						rdf:first "SubPrime" ;
						rdf:rest rdf:nil ;
					] ;
				] ;
			] ;
		] ;
	] ;
	.

fibo-sec-dbt-ab-cdo:HybridCDO
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-ab-cdo:CDODeal ,
